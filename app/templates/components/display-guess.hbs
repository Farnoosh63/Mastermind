<h2>Previous Guesses:</h2>

<table style="width:50%" id="user-guesses">
  <td id="guessPadding">
    {{#each storingData.userGuess as |guess|}}
      <tr>{{guess}}</tr>
    {{/each}}
  </td>
  <td>
    {{#each storingData.pegsArray as |pegArray|}}
      <tr>{{pegArray}}</tr>
    {{/each}}
  </td>
  {{log storingData.pegsArray}}
</table>
